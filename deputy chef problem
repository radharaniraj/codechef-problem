#include<iostream>
using namespace std;
int main()
{
    int n,t=0;
    cin>>n;
    int arr[n],a[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    for(int j=0;j<n;j++)
    {
        cin>>a[j];
    }
    for(int k=0;k<n;k++)
    {
       if(a[k]>arr[k-1]+arr[k+1])
       {
           if(a[k]>t)
           {
               t=a[k];
               cout<<t<<endl;
           }
           else
           {
               cout<<"-1"<<endl;
           }
       }
    }

}
